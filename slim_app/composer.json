{
    "name": "nerdv2/slim4-skeleton",
    "description": "Slim 4 Skeleton",
    "require": {
        "php": "^7.4",
        "lcobucci/jwt": "^4.0",
        "pecee/pixie": "^4.15",
        "pimple/pimple": "^3.2",
        "predis/predis": "^1.1",
        "selective/basepath": "^2.1",
        "slim/http-cache": "^1.1",
        "slim/psr7": "^1.0.0",
        "slim/slim": "^4.8",
        "slim/twig-view": "^3.2",
        "symfony/cache": "^5.3",
        "vlucas/phpdotenv": "^5.1"
    },
    "config": {
        "platform": {
            "php": "7.4.11"
        },
        "sort-packages": true,
        "process-timeout": 0
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "serve": [
            "cd public && php -S localhost:8080"
        ]
    }
}
